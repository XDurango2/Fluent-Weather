{
    "version": 2,
    "builds": [
      { "src": "back/background.py", "use": "@vercel/python" },
      { "src": "package.json", "use": "@vercel/static-build" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "back/background.py" },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }
  